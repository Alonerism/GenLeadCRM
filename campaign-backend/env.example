# Required for /api/session and /api/responses proxies
OPENAI_API_KEY= yes

# Optional: faster speech playback rate for Nika (client-side)
NEXT_PUBLIC_TTS_PLAYBACK_RATE=1.5

# Optional: logging verbosity (client/server)
NEXT_PUBLIC_LOG_LEVEL=info
LOG_LEVEL=info

# Google Calendar (required only if you want real calendar tools)
