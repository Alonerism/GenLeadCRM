GOOGLE_CALENDAR_CALENDAR_ID=primary
GOOGLE_CALENDAR_REDIRECT_URI=http://localhost:3000/oauth2callback

GOOGLE_GMAIL_CLIENT_ID= yes
GOOGLE_GMAIL_CLIENT_SECRET= yes
GOOGLE_GMAIL_REFRESH_TOKEN= yes
GOOGLE_GMAIL_USER=alon@nikatechsolutions.com

